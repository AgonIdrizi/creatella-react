{
  "name": "products-grid",
  "private": true,
  "version": "0.0.1",
  "description": "Build a grid of product items and ads",
  "main": "server/index.js",
  "scripts": {
    "start": "json-server server/index.js --routes server/routes.json --middlewares server/handle-delay.js server/handle-ads.js",
    "test": "jest",
    "format": "prettier --write \"src/**/*.js\" ",
    "lint": "eslint  src/*.js src/**/*.js",
    "start-client": "webpack-dev-server --mode development",
    "build": "webpack --config webpack.config.js",
    "cy:open": "cypress open",
    "dev": "concurrently --kill-others \"npm  start\" \"npm run start-client\""
  },
  "author": "",
  "license": "",
  "dependencies": {
    "axios": "^0.21.1",
    "cool-ascii-faces": "^1.3.4",
    "json-server": "^0.9.6",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.6.0",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.4",
    "antd": "^3.23.2",
    "autoprefixer": "^9.6.1",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.2",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "cypress": "^3.4.1",
    "eslint": "^6.3.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-plugin-react": "^7.14.3",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-loader": "^3.0.0",
    "prettier": "1.18.2",
    "prop-types": "^15.7.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0"
  }
}
